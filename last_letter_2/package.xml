<?xml version="1.0"?>
<package format="2">
    <name>last_letter_2</name>
    <version>0.0.0</version>
    <description>The basic pkg of Simulator. Containes all the ROS nodes</description>

    <maintainer email="tailwhipx4@gmail.com">George Zogopoulos-Papaliakos</maintainer>
    <maintainer email="milistefanos@hotmail.com">Stefanos Miliotis</maintainer>

    <license>GPL-3.0</license>

    <buildtool_depend>catkin</buildtool_depend>

    <depend>mavlink</depend>
    <depend>Eigen3</depend>

    <build_depend>roscpp</build_depend>
    <build_depend>last_letter_2_msgs</build_depend>
    <build_depend>last_letter_2_libs</build_depend>
    <build_depend>tf2_geometry_msgs</build_depend>

    <build_export_depend>roscpp</build_export_depend>
    <build_export_depend>last_letter_2_msgs</build_export_depend>
    <build_export_depend>last_letter_2_libs</build_export_depend>
    <build_export_depend>tf2_geometry_msgs</build_export_depend>

    <exec_depend>roscpp</exec_depend>
    <exec_depend>last_letter_2_msgs</exec_depend>
    <exec_depend>last_letter_2_libs</exec_depend>
    <exec_depend>tf2_geometry_msgs</exec_depend>
</package>
